[Desktop Entry]
Name=_("Distort")
Comment=_("Distort the screen")
Exec=xscreensaver-command -activate
TryExec=distort
Terminal=0
Type=Application

[Screensaver Data]
WindowIdCommand=-window-id
RootCommand=-root
Icon=distort.png
Author=
ExtendedComment=_("This screensaver does distortions of some areas of the screen, as if some object was moving under the image")
Demo=distort -delay 2

