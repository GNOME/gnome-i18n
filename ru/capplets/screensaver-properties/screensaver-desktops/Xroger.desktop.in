[Desktop Entry]
Name=_("Xroger")
Comment=_("The  xroger  program displays a replacement for the X logo.")
Exec=xscreensaver-command -activate
TryExec=xroger
Terminal=0
Type=Application

[Screensaver Data]
WindowIdCommand=-window-id
RootCommand=-root
Icon=xroger.png
Author=Jamie Zawinski
ExtendedComment=_("The  xroger  program displays a replacement for the X logo with a more accurate Look and Feel.")
Demo=xroger -delay 2

[Arg0]
Name=_("Monochrome")
Type=Check
Flag=-mono
Flag2=
LeftLabel=_("Display screensaver in monochrome.")
Comment=_("Change to display in monochrome.")