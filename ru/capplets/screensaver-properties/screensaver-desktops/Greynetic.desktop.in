[Desktop Entry]
Name=_("Greynetic")
Comment=_("Draws random color rectangles.")
Exec=xscreensaver-command -activate
TryExec=greynetic
Terminal=0
Type=Application

[Screensaver Data]
WindowIdCommand=-window-id
RootCommand=-root
Icon=greynetic.png
Author=Jamie Zawinski <jwz@jwz.org>
ExtendedComment=_("Greynetic draws random rectangles.")
Demo=greynetic -delay 2

[Arg0]
Name=_("Delay")
Type=LRange
Flag=-delay
LeftLabel=_("Faster")

RightLabel=_("Slower")

ValType=I[1-500000]=2
Label=_("Speed of Motion.")

Comment=_("Change the delay in cycles.")

[Arg1]
Name=_("Monochrome")
Type=Check
Flag=-mono
Flag2=
LeftLabel=_("Display screensaver in monochrome.")

Comment=_("Change to display in monochrome.")