#: src/resources.c:1610
# SUN NEW TRANSLATION
msgid ""
"\n"
"\tmeter is one of:\n"
"             cpu, pkts, page, swap, intr, disk, cntxt, load, colls, errs\n"
"\n"
"             Keyboard accelerators:\n"
"\n"
"             a - show all meters\n"
"             c - show CPU\n"
"             l - show load\n"
"             d - show disk\n"
"             g - show page\n"
"             x - show cntx\n"
"             w - show swap\n"
"             i - show intr\n"
"             k - show pkts\n"
"             o - show coll\n"
"             e - show errs\n"
"             m - toggle menubar\n"
"             n - toggle monitoring mode (local/remote)\n"
"             s - toggle graph style\n"
"             - - toggle showing the limit line\n"
"             t - toggle writing to the log file\n"
"             y - toggle orientation (horizontal/vertical)\n"
"             r - toggle showing strip chart\n"
"             p - show properties\n"
"             q - quit perfmeter\n"
"             1-9 - set sampletime to a range from 1-9 seconds\n"
"             + - increment the sample time by 1 second\n"
"             - - decrement the sample time by 1 second\n"
msgstr ""
"\n"
"\tmeter ist eins von:\n"
"             cpu, pkts, page, swap, intr, disk, cntxt, load, colls, errs\n"
"\n"
"             Tastaturkurzbefehle:\n"
"\n"
"             a - alle Anzeigen\n"
"             c - CPU anzeigen\n"
"             l - load anzeigen\n"
"             d - disk anzeigen\n"
"             g - page anzeigen\n"
"             x - cntx anzeigen\n"
"             w - swap anzeigen\n"
"             i - intr anzeigen\n"
"             k - pkts anzeigen\n"
"             o - coll anzeigen\n"
"             e - errs anzeigen\n"
"             m - Menüleiste anzeigen\n"
"             n - Überwachungsmodus umschalten (lokal/remote)\n"
"             s - Diagrammstil ein/aus\n"
"             - - Begrenzungslinie ein/aus\n"
"             t - Protokollaufzeichnung ein/aus\n"
"             y - Ausrichtung umschalten (horizontal/vertikal)\n"
"             r - Liniendiagramm ein/aus\n"
"             p - Eigenschaften anzeigen\n"
"             q - perfmeter beenden\n"
"             1-9 - Sample-Zeit von 1-9 Sekunden\n"
"             + - Sample-Zeit um eine Sekunde erhöhen\n"
"             - - Sample-Zeit um eine Sekunde verringern\n"

