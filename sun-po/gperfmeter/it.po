#: src/resources.c:1610
# SUN NEW TRANSLATION
msgid ""
"\n"
"\tmeter is one of:\n"
"             cpu, pkts, page, swap, intr, disk, cntxt, load, colls, errs\n"
"\n"
"             Keyboard accelerators:\n"
"\n"
"             a - show all meters\n"
"             c - show CPU\n"
"             l - show load\n"
"             d - show disk\n"
"             g - show page\n"
"             x - show cntx\n"
"             w - show swap\n"
"             i - show intr\n"
"             k - show pkts\n"
"             o - show coll\n"
"             e - show errs\n"
"             m - toggle menubar\n"
"             n - toggle monitoring mode (local/remote)\n"
"             s - toggle graph style\n"
"             - - toggle showing the limit line\n"
"             t - toggle writing to the log file\n"
"             y - toggle orientation (horizontal/vertical)\n"
"             r - toggle showing strip chart\n"
"             p - show properties\n"
"             q - quit perfmeter\n"
"             1-9 - set sampletime to a range from 1-9 seconds\n"
"             + - increment the sample time by 1 second\n"
"             - - decrement the sample time by 1 second\n"
msgstr ""
"\n"
"\tmisura uno dei parametri seguenti:\n"
"             cpu, pacc, pag, swap, intr, disc, cnts, car, coll, err\n"
"\n"
"             Scelte rapide da tastiera:\n"
"\n"
"             a - mostra tutti i parametri\n"
"             c - mostra CPU\n"
"             l - mostra carico\n"
"             d - mostra disco\n"
"             g - mostra pagine\n"
"             x - mostra contesti\n"
"             w - mostra swap\n"
"             i - mostra intr\n"
"             k - mostra pacchetti\n"
"             o - mostra collisioni\n"
"             e - mostra errori\n"
"             m - attiva/disattiva la barra dei menu\n"
"             n - attiva/disattiva la modalità di controllo (locale/remota)\n"
"             s - attiva/disattiva lo stile grafico\n"
"             - - attiva/disattiva la visualizzazione del limite\n"
"             t - attiva/disattiva la scrittura del file di log\n"
"             y - cambia l'orientamento (orizzontale/verticale)\n"
"             r - attiva/disattiva la visualizzazione dei diagrammi\n"
"             p - mostra proprietà\n"
"             q - esce dalla misurazione prestazioni\n"
"             1-9 - imposta l'intervallo di campionamento tra 1 e 9 secondi\n"
"             + - aumenta l'intervallo di campionamento di 1 secondo\n"
"             - - diminuisce l'intervallo di campionamento di 1 secondo\n"

