#: src/resources.c:1610
# SUN NEW TRANSLATION
msgid ""
"\n"
"\tmeter is one of:\n"
"             cpu, pkts, page, swap, intr, disk, cntxt, load, colls, errs\n"
"\n"
"             Keyboard accelerators:\n"
"\n"
"             a - show all meters\n"
"             c - show CPU\n"
"             l - show load\n"
"             d - show disk\n"
"             g - show page\n"
"             x - show cntx\n"
"             w - show swap\n"
"             i - show intr\n"
"             k - show pkts\n"
"             o - show coll\n"
"             e - show errs\n"
"             m - toggle menubar\n"
"             n - toggle monitoring mode (local/remote)\n"
"             s - toggle graph style\n"
"             - - toggle showing the limit line\n"
"             t - toggle writing to the log file\n"
"             y - toggle orientation (horizontal/vertical)\n"
"             r - toggle showing strip chart\n"
"             p - show properties\n"
"             q - quit perfmeter\n"
"             1-9 - set sampletime to a range from 1-9 seconds\n"
"             + - increment the sample time by 1 second\n"
"             - - decrement the sample time by 1 second\n"
msgstr ""
"\n"
"\tmeter is one of:\n"
"             cpu, pkts, page, swap, intr, disk, cntxt, load, colls, errs\n"
"\n"
"             Teclas aceleradoras del teclado:\n"
"\n"
"             a - muestra todos los medidores\n"
"             c - muestra la CPU\n"
"             l - muestra la carga\n"
"             d - muestra el disco\n"
"             g - muestra la página\n"
"             x - muestra cntx\n"
"             w - muestra el espacio de intercambio\n"
"             i - muestra intr\n"
"             k - muestra los paquetes\n"
"             o - muestra las colisiones\n"
"             e - muestra los errores\n"
"             m - conmuta la barra de menús\n"
"             n - conmuta los modos de supervisión (local/remoto)\n"
"             s - conmuta los estilos de gráficos\n"
"             - - conmuta cómo mostrar la línea límite\n"
"             t - conmuta la escritura en el archivo de registro\n"
"             y - conmuta la orientación (horizontal/vertical)\n"
"             r - conmuta la visualización de la cinta de registro\n"
"             p - muestra las propiedades\n"
"             q - cierra el medidor de rendimiento\n"
"             1-9 - establece el tiempo de muestra de 1 a 9 segundos\n"
"             + - incrementa el tiempo de muestra en 1 segundo\n"
"             - - disminuye el tiempo de muestra en 1 segundo\n"

