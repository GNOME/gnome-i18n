msgid "1/1/00, 1:00 AM"
msgstr "1/1/00, 1:00 上午"

#. The third one is "%_m/%_d/%_y, %_I:%M %p".
#: eel/eel-glib-extensions.c:1132
msgid " 1/ 1/00,  1:00 AM"
msgstr " 1/ 1/00,  1:00 上午"

