msgid "Service"
msgstr "サービス"

#: src/firestarter.c:557
msgid "Time"
msgstr "時間"

#: src/firestarter.c:559
msgid "Deny all connections from"
msgstr "禁止する接続"

#: src/firestarter.c:560
msgid "Allow all connections from"
msgstr "許可する接続"

#: src/firestarter.c:561 src/menus.c:89
msgid "Open service to machine"
msgstr "マシンに有効なサービス"

#: src/firestarter.c:562 src/menus.c:90
