msgid "_Firewall"
msgstr "ファイヤーウォール(_F)"

#: src/menus.c:69
msgid "_Hit list"
msgstr "ヒットリスト(_H)"

#: src/menus.c:75
msgid "Run firewall wizard"
msgstr "ウィザードを起動します"

#: src/menus.c:77
msgid "Clear firewall hit list"
msgstr "ヒットリストを消去します"

#: src/menus.c:78
msgid "Reload entire firewall hit list"
msgstr "ヒットリストの全てを再度読み込みます"

#: src/menus.c:79
msgid "Save firewall hit list to file"
msgstr "ヒットリストをファイルに保存します"

#: src/menus.c:81
msgid "Restart firewall"
msgstr "ファイヤーウォールを再起動します"

#: src/menus.c:82
msgid "Stop firewall"
msgstr "ファイヤーウォールを停止します"

#: src/menus.c:87
msgid "Deny all connections from machine"
msgstr "全ての接続を禁止"

#: src/menus.c:88
msgid "Allow all connections from machine"
msgstr "全ての接続を許可"

#: src/menus.c:92
msgid "Block and stop logging this port"
msgstr "このポートを閉じてログを停止します"

#: src/menus.c:93
msgid "Lookup hostname"
msgstr "ホスト名の検索"

#: src/menus.c:98
msgid "Remove this rule"
msgstr "このルールを削除"

#: src/menus.c:99
msgid "Remove all rules in this group"
msgstr "このグループから全てのルールを削除"

#: src/menus.c:100
msgid "Remove all rules in all groups"
msgstr "全てのグループの全てのルールを削除"

