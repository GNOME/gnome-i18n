msgid "R_estart firewall"
msgstr "再起動(_E)"

#: src/menus.c:33 src/menus.c:44
msgid "_Stop firewall"
msgstr "停止(_S)"

#: src/menus.c:42
msgid "_Run firewall wizard"
msgstr "ウィザード起動(_R)"

#: src/menus.c:45
msgid "_Halt all network traffic"
msgstr "全ネットワーク通信を停止(_H)"

#: src/menus.c:53
msgid "_Clear firewall hit list"
msgstr "クリア(_C)"

#: src/menus.c:54
msgid "R_eload entire firewall hit list"
msgstr "再読み込み(_E)"

#: src/menus.c:55
msgid "_Save firewall hit list to file"
msgstr "保存(_S)"

#: src/menus.c:61
msgid "Firestarter _Homepage"
msgstr "Firestarter ホームページ(_H)"

#: src/menus.c:68
msgid "_Firewall"
msgstr "ファイヤーウォール(_F)"

#: src/menus.c:69
msgid "_Hit list"
msgstr "ヒットリスト(_H)"

#: src/menus.c:75
msgid "Run firewall wizard"
msgstr "ウィザードを起動します"

