#: daemon/gdm.c:166
#, c-format
msgid "gdm_config_parse: No configuration file: %s. Aborting."
msgstr "gdm_config_parse: Nincs %s beállítás fájl. Mûvelet megszakítása."

#: daemon/gdm.c:217
msgid "gdm_config_parse: No greeter specified and default not found."
msgstr "gdm_config_parse: Az üdvözlõ üzenet nincs megadva és nincs "
"alapértelmezett."

#: daemon/gdm.c:229
msgid "gdm_config_parse: No authdir specified and default not found."
msgstr "gdm_config_parse: A jogkönyvtár nincs megadva és nincs "
"alapértelmezett."

#: daemon/gdm.c:247
msgid ""
"gdm_config_parse: No sessions directory specified and default not found."
msgstr "gdm_config_parse: A session könyvtár nincs megadva és nincs "
"alapértelmezett."

#: daemon/gdm.c:259
msgid "gdm_config_parse: Invalid server line in config file. Ignoring!"
msgstr ""
"gdm_config_parse: Érvénytelen szerver sor a beállításokban. Figyelmen kívül "
"hagytam!"

#: daemon/gdm.c:265
msgid ""
"gdm_config_parse: Xdmcp disabled and no local servers defined. Aborting!"
msgstr ""
"gdm_config_parse: Az xdmcp ki van kapcsolva és nincs helyi szerver megadva. "
"Mûvelet megszakítása."

#: daemon/gdm.c:270
#, c-format
msgid "gdm_config_parse: Can't find the gdm user (%s). Aborting!"
msgstr ""
"gdm_config_parse: Nem találom a gdm felhasználóját (%s). Mûvelet "
"megszakítása."

#: daemon/gdm.c:275
msgid "gdm_config_parse: The gdm user should not be root. Aborting!"
msgstr ""
"gdm_config_parse: A gdm felhasználó nem lehetne a root. Mûvelet megszakítása."

#: daemon/gdm.c:280
#, c-format
msgid "gdm_config_parse: Can't find the gdm group (%s). Aborting!"
msgstr ""
"gdm_config_parse: Nem találom a gdm csoportját (%s). Mûvelet megszakítása."

#: daemon/gdm.c:285
msgid "gdm_config_parse: The gdm group should not be root. Aborting!"
msgstr ""
"gdm_config_parse: A gdm csoportja nem lehetne root. Mûvelet megszakítása."

#: daemon/gdm.c:292
#, c-format
msgid "gdm_config_parse: Authdir %s does not exist. Aborting."
msgstr "gdm_config_parse: A %s jogkönyvtár nem létezik. Mûvelet megszakítása."

#: daemon/gdm.c:295
#, c-format
msgid "gdm_config_parse: Authdir %s is not a directory. Aborting."
msgstr "gdm_config_parse: A %s jogkönyvtár nem könyvtár. Mûvelet megszakítása."

#: daemon/gdm.c:298
#, c-format
msgid ""
"gdm_config_parse: Authdir %s is not owned by user %s, group %s. Aborting."
msgstr ""
"gdm_config_parse: A %s jogkönyvtár nem %s felhasználó és %s csoport "
"tulajdonában van. Mûvelet megszakítása."

#: daemon/gdm.c:302
#, c-format
msgid ""
"gdm_config_parse: Authdir %s has wrong permissions. Should be 750. Aborting."
msgstr ""
"gdm_config_parse: A %s jogkönyvtár jogai rosszak. 750 kellene legyen. "
"Mûvelet megszakítása."

#: daemon/gdm.c:363
#, c-format
