#: lib/getopt.c:681
#, c-format
msgid "%s: option `%s' is ambiguous\n"
msgstr "%s : l'option « %s » est ambigüe\n"

#: lib/getopt.c:706
#, c-format
msgid "%s: option `--%s' doesn't allow an argument\n"
msgstr "%s : l'option « --%s » n'accepte pas de paramètre\n"

