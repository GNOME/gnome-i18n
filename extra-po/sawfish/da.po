# ../lisp/cycle.jl
#. ../lisp/cycle.jl
msgid "Select the next window of the current workspace."
msgstr "Vælg næste vindue på nuværende skrivebord."

# ../lisp/cycle.jl
#. ../lisp/cycle.jl
msgid "Focus on the previous window of the current workspace."
msgstr "Fokusér på forrige vindue på nuværende skrivebord."

# ../lisp/cycle.jl
#. ../lisp/cycle.jl
msgid "Select the next window, cycling through all possible workspaces."
msgstr "Vælg det næste vindue, gennemgå alle mulige skriveborde."

# ../lisp/cycle.jl
#. ../lisp/cycle.jl
msgid "Select the previous window, cycling through all possible workspaces."
msgstr "Vælg det forrige vindue, gennemgå alle mulige skriveborde."

# ../lisp/edge-flip.jl
#. ../lisp/edge-flip.jl
msgid "Edge Flipping"
msgstr "Kant-skiftning"

# ../lisp/edge-flip.jl
#. ../lisp/edge-flip.jl
msgid "Flip to next viewport/workspace when pointer hits screen edge."
msgstr "Spring til næste visport/skrivebord, når markør rammer skærmkant."

#. ../lisp/edge-flip.jl
msgid "Only flip when interactively moving a window."
msgstr "Spring kun under interaktiv flytning af vinduer."

# ../lisp/edge-flip.jl
#. ../lisp/edge-flip.jl
msgid "What hitting the screen edge actually flips."
msgstr "Hvad får skærmen til at skifte, når det rører kanten?"

# ../lisp/edge-flip.jl
#. ../lisp/edge-flip.jl
msgid ""
"Number of milliseconds to wait after pointer hits screen edge before\n"
"flipping."
msgstr ""
"Antal millisekunders ventetid efter markør rører skærmkanten\n"
"før skærmskift"

# ../lisp/focus.jl
#. ../lisp/focus.jl
msgid "When does the mouse pointer affect the input focus."
msgstr "Hvornår påvirker musemarkøren inddata fokusen."

# ../lisp/focus.jl
#. ../lisp/focus.jl
msgid "Does click-to-focus mode pass the click through to the window."
msgstr "Videregiver klik-for-fokus klikket til vinduet."

# ../lisp/frames.jl
#. ../lisp/frames.jl
msgid "Default frame style (theme)."
msgstr "Standard-rammestil (tema)."

# ../lisp/frames.jl
#. ../lisp/frames.jl
msgid "Update all windows when the default frame style is changed."
msgstr "Opdatér alle vinduer, når forvalgt rammestil er ændret."

# ../lisp/frames.jl
#. ../lisp/frames.jl
msgid "Decorate transient windows similarly to top-level windows."
msgstr "Dekorér transistente vinduer på samme måde som top-niveau vinduer."

# ../lisp/frames.jl
#. ../lisp/frames.jl
msgid "Automatically reload themes when they are updated."
msgstr "Automatisk genindlæs temaer når de er opdateret."

# ../lisp/frames.jl
#. ../lisp/frames.jl
msgid "Default"
msgstr "Standard"

# ../lisp/functions.jl
#. ../lisp/functions.jl
msgid "Give focus to windows even when they haven't asked for it."
msgstr "Giv fokus til vinduer selvom de ikke har bedt om det."

#. ../lisp/functions.jl
msgid ""
"Offset in percent from left window edge, when warping.\n"
"A negative number means warp to outside the left window edge."
msgstr ""
"Afstanden i procent fra venstre vindueskant, ved warping\n"
"Et negativt tal betyder warp til yderkanten af venstre vindue."

#. ../lisp/functions.jl
msgid ""
"Offset in percent from top window edge, when warping.\n"
"A negative number means warp to outside the top window edge."
msgstr ""
"Afstanden i procent fra top vindueskanten, ved warping\n"
"Et negativt tal betyder warp til yderkanten af top vinduet."

