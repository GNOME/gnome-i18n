msgstr "Ikkunoiden kierrätys"

#. ../lisp/x-cycle.jl
msgid "Display window names while cycling through windows."
msgstr "Näytä ikkunoiden nimet kierrättäessä."

#. ../lisp/x-cycle.jl
msgid "Include iconified windows when cycling."
msgstr "Huomioi myös piilotetut ikkunat kierrätyksessä."

#. ../lisp/x-cycle.jl
msgid "Include windows on all workspaces when cycling."
msgstr "Huomioi kaikkien työpöytien ikkunat kierrätyksessä."

#. ../lisp/x-cycle.jl
msgid "Include windows on all viewports when cycling."
msgstr "Huomioi kaikkien alityöpöytien (viewports) ikkunat kierrätyksessä."

#. ../lisp/x-cycle.jl
msgid "Raise windows while they're temporarily selected during cycling."
msgstr "Nosta ikkunat kun ne väliaikaisesti valitaan kierrättäessä."

#. ../lisp/x-cycle.jl
msgid "Warp the mouse pointer to windows as they're temporarily selected."
msgstr "Siirrä hiiren osoitin ikkunoihin kun ne valitaan kierrättäessä."

#. ../lisp/x-cycle.jl
msgid "Focus windows when they're temporarily selected during cycling."
msgstr "Kohdista ikkunat kierrättäessä."

#. ../lisp/x-cycle.jl
msgid "Disable auto-raising while temporarily selecting windows."
msgstr "Poista automaattinosto käytöstä ikkunoita valitessa."

#. ../lisp/x-cycle.jl
msgid "Cycle through all windows in order of recent selections."
msgstr ""

#. ../lisp/x-cycle.jl
msgid "Cycle through all windows in the same group as the current window."
msgstr "Kierrätä senhetkisen ikkunan kanssa samassa ryhmässä olevia ikkunoita."

#. ../lisp/x-cycle.jl
msgid ""
"Cycle through all windows whose names match the leading colon-delimited\n"
"prefix of the current window."
msgstr ""

#. ../lisp/x-cycle.jl
msgid "Cycle through all windows with the same class as the current window."
msgstr ""

