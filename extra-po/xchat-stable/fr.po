#: src/common/outbound.c:167
msgid "No channel joined. Try /join #<channel>\n"
msgstr "Pas encore dans un canal. Essayez /join #<canal>\n"

#: src/common/outbound.c:173
msgid "Not connected. Try /server <host> [<port>]\n"
msgstr "Pas encore connecté. Essayez /server <hote> [<port>]\n"

#: src/common/outbound.c:477
msgid "/ADDBUTTON <name> <action>, adds a button under the user-list\n"
msgstr ""
"/ADDBUTTON <nom> <action>, ajoute un bouton en-dessous de la liste des "
"utilisateurs\n"

#: src/common/outbound.c:479
msgid "/ALLCHAN <cmd>, sends a command to all channels you're in\n"
msgstr ""
"/ALLCHAN <cmd>, envoie une commande à tous les canaux dans lesquels vous "
"êtes présent\n"

#: src/common/outbound.c:481
msgid "/ALLSERV <cmd>, sends a command to all servers you're in\n"
msgstr ""
"/ALLSERV <cmd>, envoie une commande à tous les serveurs sur lesquels vous "
"êtes connecté\n"

#: src/common/outbound.c:482
msgid "/AWAY [<reason>], sets you away\n"
msgstr "/AWAY [<raison>], vous signale comme absent\n"

#: src/common/outbound.c:484
msgid ""
"/BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)\n"
msgstr ""
"/BAN <masque> [<typeban>], banit tous ceux qui correspondent au masque dans "
"le canal en cours. S'ils sont déjà sur le canal ça ne les met pas dehors "
"(kick) (nécessite chanop)\n"

#: src/common/outbound.c:486
msgid "/UNBAN <mask> [<mask>...], unbans the specified masks.\n"
msgstr "/UNBAN <masque> [<masque>...], unban le masques fournis.\n"

#: src/common/outbound.c:487
msgid "/CLEAR, Clears the current text window\n"
msgstr "/CLEAR, Vide la fenêtre de texte courante\n"

#: src/common/outbound.c:488
msgid "/CLOSE, Closes the current window/tab\n"
msgstr "/CLOSE, Ferme l'onglet/fenêtre courant\n"

#: src/common/outbound.c:491
msgid ""
"/CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO\n"
msgstr ""
"/CTCP <pseudo> <message>, envoie le message CTCP au pseudo, les messages "
"courants sont VERSION et USERINFO\n"

#: src/common/outbound.c:493
msgid "/COUNTRY <code>, finds a country code, eg: au = australia\n"
msgstr ""
"/COUNTRY <code>, recherche un code de pays, par exemple : au = australie\n"

#: src/common/outbound.c:495
msgid "/CYCLE, parts current channel and immediately rejoins\n"
msgstr "/CYCLE, sort du canal courant et revient immédiatement\n"

#: src/common/outbound.c:497
msgid ""
"\n"
"/DCC GET <nick>          - receive an offered file\n"
"/DCC SEND <nick> <file>  - send a file to someone\n"
"/DCC LIST                - show DCC list\n"
"/DCC CHAT <nick>         - offer DCC CHAT to someone\n"
"/DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz\n"
msgstr ""
"\n"
"/DCC GET <pseudo>             - reçoit un fichier proposé\n"
"/DCC SEND <pseudo> <fichier>  - envoie un fichier à quelqu'un\n"
"/DCC LIST                     - affiche la liste DCC\n"
"/DCC CHAT <pseudo>            - propose un DCC CHAT à quelqu'un\n"
"/DCC CLOSE <type> <pseudo> <fichier>     exemple:\n"
"         /dcc close send rogerdupont fichier.tar.gz\n"

#: src/common/outbound.c:508
msgid "/DELBUTTON <name>, deletes a button from under the user-list\n"
msgstr ""
"/DELBUTTON <nom>, supprime un bouton de sous la liste des utilisateurs\n"

#: src/common/outbound.c:510
msgid ""
"/DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)\n"
msgstr ""
"/DEHOP <pseudo>, supprime le statut chanhalf-op d'un pseudo sur le canal "
"courant (nécessite chanop)\n"

#: src/common/outbound.c:512
msgid ""
"/DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)\n"
msgstr ""
"/DEOP <pseudo>, supprime le statut chanop d'un pseudo sur le canal courant "
"(nécessite chanop)\n"

#: src/common/outbound.c:514
msgid ""
"/DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)\n"
msgstr ""
"/DEVOICE <pseudo>, supprime le statut voice d'un pseudo sur le canal courant "
"(nécessite chanop)\n"

#: src/common/outbound.c:515
msgid "/DISCON, Disconnects from server\n"
msgstr "/DISCON, Se déconnecte du serveur\n"

#: src/common/outbound.c:516
msgid "/DNS <nick|host|ip>, Finds a users IP number\n"
msgstr "/DNS <pseudo|hote|ip>, trouve l'adresse IP d'un utilisateur\n"

#: src/common/outbound.c:517
msgid "/ECHO <text>, Prints text locally\n"
msgstr "/ECHO <texte>, Affiche un texte en local\n"

#: src/common/outbound.c:520
msgid ""
"/EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box\n"
msgstr ""
"/EXEC [-o] <commande>, exécute une commande. Si l'option -o est utilisée, la "
"sortie est envoyée sur le canal courant, sinon elle est affichée dans la "
"zone de texte courante\n"

#: src/common/outbound.c:522
msgid ""
"/EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed\n"
msgstr ""
"/EXECKILL [-9], tue une exécution en cours dans la session courante. Si -9 "
"est fourni, le processus est tué par SIGKILL\n"

#: src/common/outbound.c:524
msgid "/EXECSTOP, sends the process SIGSTOP\n"
msgstr "/EXECSTOP, envoie un SIGSTOP au processus\n"

#: src/common/outbound.c:525
msgid "/EXECCONT, sends the process SIGCONT\n"
msgstr "/EXECCONT, envoie un SIGCONT au processus\n"

#: src/common/outbound.c:526
msgid "/EXECWRITE, sends data to the processes stdin\n"
msgstr "/EXECWRITE, envoie des données à l'entrée standard du processus\n"

#: src/common/outbound.c:530
msgid "/FLUSHQ, flushes the current server's send queue\n"
msgstr "/FLUSHQ, vide la queue d'envoi du serveur courant\n"

#: src/common/outbound.c:532
msgid "/GATE <host> [<port>], proxies through a host, port defaults to 23\n"
msgstr ""
"/GATE <hote> [<port>], utilise un hote comme proxy, le port est 23 par "
"défaut\n"

#: src/common/outbound.c:535
msgid "/HOP <nick>, gives chanhalf-op status to the nick (needs chanop)\n"
msgstr ""
"/HOP <pseudo>, donne le statut chanhalf-op au pseudo (nécessite chanop)\n"

#: src/common/outbound.c:537
msgid ""
"/IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    options - NOSAVE, QUIET\n"
msgstr ""
"/IGNORE <masque> <types..> <options..>\n"
"    masque  - masque d'hotes à ignorer, par ex.: *!*@*.aol.com\n"
"    types   - types de données à ignorer, parmi (ou tous) :\n"
"              PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    options - NOSAVE, QUIET\n"

#: src/common/outbound.c:544
msgid ""
"/INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)\n"
msgstr ""
"/INVITE <pseudo> [<canal>], invite quelqu'un sur un canal, par défaut le "
"canal courant (nécessite chanop)\n"

#: src/common/outbound.c:545
msgid "/JOIN <channel>, joins the channel\n"
msgstr "/JOIN <canal>, rejoint le canal\n"

#: src/common/outbound.c:547
msgid "/KICK <nick>, kicks the nick from the current channel (needs chanop)\n"
msgstr ""
"/KICK <pseudo>, kicke le pseudo (met dehors) du canal courant (nécessite "
"chanop)\n"

#: src/common/outbound.c:549
msgid ""
"/KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)\n"
msgstr ""
"/KICKBAN <pseudo>, bannit puis kicke (met dehors) le pseudo du canal courant "
"(nécessite chanop)\n"

#: src/common/outbound.c:551
msgid "/LAGCHECK, forces a new lag check\n"
msgstr "/LAGCHECK, force une nouvelle vérification de lag (délai)\n"

#: src/common/outbound.c:553
msgid "/LASTLOG <string>, searches for a string in the buffer.\n"
msgstr "/LASTLOG <chaine>, recherche une chaine dans le buffer (tampon).\n"

#: src/common/outbound.c:557
msgid "/LISTDLL, Lists all currenly loaded plugins\n"
msgstr "/LISTDLL, liste tous les greffons actuellement chargés\n"

#: src/common/outbound.c:559
msgid "/LOAD <file>, loads a Perl script\n"
msgstr "/LOAD <fichier>, charge un script Perl\n"

#: src/common/outbound.c:561
msgid "/LOADDLL <file>, loads a plugin\n"
msgstr "/LOADDLL <fichier>, charge un greffon\n"

#: src/common/outbound.c:565
msgid ""
"/MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)\n"
msgstr ""
"/MDEHOP, fait un deop massif sur tous les chanhalf-ops du canal courant "
"(nécessite chanop)\n"

#: src/common/outbound.c:567
msgid "/MDEOP, Mass deop's all chanops in the current channel (needs chanop)\n"
msgstr ""
"/MDEOP, fait un deop massif sur tous les chanops du canal courant (nécessite "
"chanop)\n"

#: src/common/outbound.c:569
msgid ""
"/MKICK, Mass kicks everyone except you in the current channel (needs "
"chanop)\n"
msgstr ""
"/MKICK, fait un kick massif de tout le monde sauf vous dans le canal courant "
"(nécessite chanop)\n"

#: src/common/outbound.c:571
msgid ""
"/MKICKB, Sets a ban of *@* and mass kicks everyone except you in the current "
"channel (needs chanop)\n"
msgstr ""
"/MKICKB, banit *@* et fait un kick massif de tout le monde sauf vous dans le "
"canal courant (nécessite chanop)\n"

#: src/common/outbound.c:573
msgid ""
"/ME <action>, sends the action to the current channel (actions are written "
"in the 3rd person, like /me jumps)\n"
msgstr ""
"/ME <action>, envoie l'action dans le canal courant (les actions sont "
"écrites à la 3ème personne, comme /me est content)\n"

#: src/common/outbound.c:574
msgid "/MSG <nick> <message>, sends a private message\n"
msgstr "/MSG <pseudo> <message>, envoie un message privé\n"

#: src/common/outbound.c:577
msgid "/NAMES, Lists the nicks on the current channel\n"
msgstr "/NAMES, Liste les pseudos sur le canal courant\n"

#: src/common/outbound.c:579
msgid "/NCTCP <nick> <message>, Sends a CTCP notice\n"
msgstr "/NCTCP <pseudo> <message>, Envoie une notice CTCP\n"

#: src/common/outbound.c:580
msgid "/NEWSERVER <hostname> [<port>]\n"
msgstr "/NEWSERVER <nomhote> [<port>]\n"

#: src/common/outbound.c:581
msgid "/NICK <nickname>, sets your nick\n"
msgstr "/NICK <pseudonyme>, définit votre pseudo\n"

#: src/common/outbound.c:584
msgid ""
"/NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to\n"
msgstr ""
"/NOTICE <pseudo/canal> <message>, envoie une notice. Les notices sont un "
"type de message auquel on devrait réagir automatiquement\n"

#: src/common/outbound.c:586
msgid "/NOTIFY [<nick>], lists your notify list or adds someone to it\n"
msgstr ""
"/NOTIFY [<pseudo>], liste votre liste de notifications et y ajoute "
"quelqu'un\n"

#: src/common/outbound.c:588
msgid "/OP <nick>, gives chanop status to the nick (needs chanop)\n"
msgstr "/OP <pseudo>, donne le statut chanop au pseudo (nécessite chanop)\n"

#: src/common/outbound.c:590
msgid ""
"/PART [<channel>] [<reason>], leaves the channel, by default the current "
"one\n"
msgstr ""
"/PART [<canal>] [<raison>], quitte le canal, par défaut le canal courant\n"

#: src/common/outbound.c:592
msgid "/PING <nick | channel>, CTCP pings nick or channel\n"
msgstr "/PING <pseudo | canal>, envoie un ping CTCP au pseudo ou au canal\n"

#: src/common/outbound.c:596
msgid "/PKILL <name>, kills the script of the given name\n"
msgstr "/PKILL <nom>, tue le script ayant ce nom\n"

#: src/common/outbound.c:597
msgid "/PLIST, lists the current python scripts\n"
msgstr "/PLIST, liste les scripts python en cours\n"

#: src/common/outbound.c:598
msgid "/PLOAD loads a python script\n"
msgstr "/PLOAD charge un script python\n"

#: src/common/outbound.c:602
msgid "/QUERY <nick>, opens up a new privmsg window to someone\n"
msgstr ""
"/QUERY <pseudo>, ouvre une nouvelle fenêtre de message privé avec quelqu'un\n"

#: src/common/outbound.c:604
msgid "/QUIT [<reason>], disconnects from the current server\n"
msgstr "/QUIT [<raison>], déconnecte du seveur courant\n"

#: src/common/outbound.c:606
msgid "/QUOTE <text>, sends the text in raw form to the server\n"
msgstr "/QUOTE <texte>, transmet le texte sous forme brute au serveur\n"

#: src/common/outbound.c:609
msgid ""
"/RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers\n"
msgstr ""
"/RECONNECT [-ssl] [<hote>] [<port>] [<motpasse>], Peut être appellé juste "
"avec /RECONNECT pour se reconnecter au serveur courant ou avec /RECONNECT "
"ALL pour se reconnecter à tous les serveurs ouverts\n"

#: src/common/outbound.c:612
msgid ""
"/RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers\n"
msgstr ""
"/RECONNECT [<hote>] [<port>] [<motpasse>], Peut être appellé juste avec /"
"RECONNECT pour se reconnecter au serveur courant ou avec /RECONNECT ALL pour "
"se reconnecter à tous les serveurs ouverts\n"

#: src/common/outbound.c:615
msgid "/RMDLL <dll name>, unloads a plugin\n"
msgstr "/RMDLL <nom dll>, décharge un greffon\n"

#: src/common/outbound.c:619
msgid "/SAY <text>, sends the text to the object in the current window\n"
msgstr "/SAY <texte>, envoie un texte à l'objet de la fenêtre courante\n"

#: src/common/outbound.c:622
msgid "/SCPINFO, Lists some information about current Perl bindings\n"
msgstr ""
"/SCPINFO, Liste certaines informations à propos des associations (bindings) "
"Perl actuelles\n"

#: src/common/outbound.c:624
msgid "/SET <variable> [<value>]\n"
msgstr "/SET <variable> [<valeur>]\n"

#: src/common/outbound.c:628
msgid ""
"/SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel\n"
msgstr ""
"/SERVCHAN [-ssl] <hote> <port> <canal>, se connecte et rejoint un canal\n"

#: src/common/outbound.c:631
msgid "/SERVCHAN <host> <port> <channel>, connects and joins a channel\n"
msgstr "/SERVCHAN <hote> <port> <canal>, se connecte et rejoint un canal\n"

#: src/common/outbound.c:635
msgid ""
"/SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the "
"default port is 6667 for normal connections, and 994 for ssl connections.\n"
msgstr ""
"/SERVER [-ssl] <hote> [<port>] [<motpasse>], se connecte à un serveur, le "
"port par défaut est 6667 pour les connexions normales, et 994 pour les "
"connexions ssl.\n"

#: src/common/outbound.c:638
msgid ""
"/SERVER <host> [<port>] [<password>], connects to a server, the default port "
"is 6667.\n"
msgstr ""
"/SERVER <hote> [<port>] [<motpasse>], se connecte à un serveur, le port par "
"défaut est 6667.\n"

#: src/common/outbound.c:640
msgid "/TIMER <seconds> <command>\n"
msgstr "/TIMER <secondes> <commande>\n"

#: src/common/outbound.c:642
msgid ""
"/TOPIC [<topic>], sets the topic if one is given, else shows the current "
"topic\n"
msgstr ""
"/TOPIC [<sujet>], définit le sujet si on en fourni un, sinon affiche le "
"sujet actuel\n"

#: src/common/outbound.c:643
msgid "/UNIGNORE <mask> [QUIET]\n"
msgstr "/UNIGNORE <masque> [QUIET]\n"

#: src/common/outbound.c:645
msgid "/UNLOADALL, Unloads all perl scripts\n"
msgstr "/UNLOADALL, Décharge tous les scripts perl\n"

#: src/common/outbound.c:649
msgid ""
"/WALLCHOP <message>, sends the message to all chanops on the current "
"channel\n"
msgstr ""
"/WALLCHOP <message>, envoie le message à tous les chanops du canal courant\n"

#: src/common/outbound.c:651
msgid "/WALLCHAN <message>, writes the message to all channels\n"
msgstr "/WALLCHAN <message>, écrit le message sur tous les canaux\n"

#: src/common/outbound.c:653
msgid "/VOICE <nick>, gives voice status to someone (needs chanop)\n"
msgstr ""
"/VOICE <pseudo>, donne le statut voice à quelqu'un (nécessite chanop)\n"

