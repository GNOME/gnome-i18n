#: src/common/outbound.c:167
msgid "No channel joined. Try /join #<channel>\n"
msgstr "Kein Kanal betreten. Probieren Sie /join #<Kanal>\n"

#: src/common/outbound.c:173
msgid "Not connected. Try /server <host> [<port>]\n"
msgstr "Nicht verbunden. Probieren Sie /server <Host> [<Port>]\n"

#: src/common/outbound.c:473
msgid "/ADDBUTTON <name> <action>, adds a button under the user-list\n"
msgstr ""
"/ADDBUTTON <Name> <Aktion>, fügt einen Knopf unterhalb der Benutzerliste "
"hinzu\n"

#: src/common/outbound.c:475
msgid "/ALLCHAN <cmd>, sends a command to all channels you're in\n"
msgstr ""
"/ALLCHAN <Befehl>, sendet einen Befehl in alle Kanäle, in denen Sie sich "
"aufhalten\n"

#: src/common/outbound.c:477
msgid "/ALLSERV <cmd>, sends a command to all servers you're in\n"
msgstr ""
"/ALLSERV <Befehl>, sendet einen Befehl an alle Server, zu denen Sie "
"verbunden sind\n"

#: src/common/outbound.c:478
msgid "/AWAY [<reason>], sets you away\n"
msgstr "/AWAY [<Grund>], setzt Ihren Status auf \"abwesend\" (\"away\")\n"

#: src/common/outbound.c:480
msgid ""
"/BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)\n"
msgstr ""
"/BAN <Maske> [<Bann-Typ>], verbannt jeden mit einer entsprechenden "
"Host-Maske vom aktuellen Kanal. Wenn jene sich schon im Kanal aufhalten, "
"werden sie durch diesen Befehl nicht hinausgeworfen (benötigt "
"Operator-Status)\n"

#: src/common/outbound.c:482
msgid "/UNBAN <mask> [<mask>...], unbans the specified masks.\n"
msgstr ""
"/UNBAN <Maske> [<Maske>...], hebt einen Bann auf den angegebenen Host-Masken "
"auf.\n"

#: src/common/outbound.c:483
msgid "/CLEAR, Clears the current text window\n"
msgstr "/CLEAR, löscht das aktuelle Textfenster\n"

#: src/common/outbound.c:484
msgid "/CLOSE, Closes the current window/tab\n"
msgstr "/CLOSE, schließt das aktuelle Fenster bzw. den aktuellen Reiter\n"

#: src/common/outbound.c:487
msgid ""
"/CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO\n"
msgstr ""
"/CTCP <Spitzname> <Nachricht>, sendet die CTCP-Nachricht an den Spitznamen, "
"übliche Nachrichten sind VERSION und USERINFO\n"

#: src/common/outbound.c:489
msgid "/COUNTRY <code>, finds a country code, eg: au = australia\n"
msgstr ""
"/COUNTRY <Code>, findet das Land zu einem Ländercode, z.B. au = australia\n"

#: src/common/outbound.c:491
msgid "/CYCLE, parts current channel and immediately rejoins\n"
msgstr "/CYCLE, verläßt den aktuellen Kanal und betritt ihn sofort wieder\n"

#: src/common/outbound.c:493
msgid ""
"\n"
"/DCC GET <nick>          - receive an offered file\n"
"/DCC SEND <nick> <file>  - send a file to someone\n"
"/DCC LIST                - show DCC list\n"
"/DCC CHAT <nick>         - offer DCC CHAT to someone\n"
"/DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz\n"
msgstr ""
"\n"
"/DCC GET <Spitzname>          - eine angebotene Datei empfangen\n"
"/DCC SEND <Spitzname> <Datei> - eine Datei an jemanden senden\n"
"/DCC LIST                     - DCC-Liste anzeigen\n"
"/DCC CHAT <Spitzname>         - jemandem einen DCC-Dialog anbieten\n"
"/DCC CLOSE <Typ> <Spitzname> <Datei> - beendet eine DCC-Verbindung\n"
"                  Beispiel: /dcc close send karlmaier datei.tar.gz\n"

#: src/common/outbound.c:504
msgid "/DELBUTTON <name>, deletes a button from under the user-list\n"
msgstr ""
"/DELBUTTON <Name>, löscht einen der Knöpfe unterhalb der Benutzerliste\n"

#: src/common/outbound.c:506
msgid ""
"/DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)\n"
msgstr ""
"/DEHOP <Spitzname>, entfernt Halb-Operator-Status (\"chanhalf-op\") von "
"diesem Spitznamen im aktuellen Kanal (benötigt Operator-Status)\n"

#: src/common/outbound.c:508
msgid ""
"/DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)\n"
msgstr ""
"/DEOP <Spitzname>, entfernt Operator-Status von diesem Spitznamen im "
"aktuellen Kanal (benötigt Operator-Status)\n"

#: src/common/outbound.c:510
msgid ""
"/DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)\n"
msgstr ""
"/DEVOICE <Spitzname>, entfernt Voice-Status von diesem Spitzname im "
"aktuellen Kanal (benötigt Operator-Status)\n"

#: src/common/outbound.c:511
msgid "/DISCON, Disconnects from server\n"
msgstr "/DISCON, trennt die Verbindung zum Server\n"

#: src/common/outbound.c:512
msgid "/DNS <nick|host|ip>, Finds a users IP number\n"
msgstr "/DNS <Spitzname|Host|IP>, findet die IP-Nummer eines Benutzers\n"

#: src/common/outbound.c:513
msgid "/ECHO <text>, Prints text locally\n"
msgstr "/ECHO <Text>, gibt den Text lokal aus\n"

#: src/common/outbound.c:516
msgid ""
"/EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box\n"
msgstr ""
"/EXEC [-o] <Befehl>, führt einen Befehl aus. Wenn die »-o«-Option verwendet "
"wird, dann wird die Ausgabe zum aktuellen Kanal gesendet, ansonsten in das "
"aktuelle Textfeld\n"

#: src/common/outbound.c:518
msgid ""
"/EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed\n"
msgstr ""
"/EXECKILL [-9], beendet einen laufenden (über »/EXEC« gestarteten) Befehl. "
"Wurde »-9« angegeben, wird dem Prozess ein »SIGKILL« gesendet\n"

#: src/common/outbound.c:520
msgid "/EXECSTOP, sends the process SIGSTOP\n"
msgstr "/EXECSTOP, sendet dem Prozess ein »SIGSTOP«\n"

#: src/common/outbound.c:521
msgid "/EXECCONT, sends the process SIGCONT\n"
msgstr "/EXECSTOP, sendet dem Prozess ein »SIGCONT«\n"

#: src/common/outbound.c:522
msgid "/EXECWRITE, sends data to the processes stdin\n"
msgstr "/EXECWRITE, sendet Daten zur Standardeingabe (»stdin«) des Prozesses\n"

#: src/common/outbound.c:526
msgid "/FLUSHQ, flushes the current server's send queue\n"
msgstr "/FLUSHQ, entleert die Sendewarteschlange der aktuellen Servers\n"

#: src/common/outbound.c:528
msgid "/GATE <host> [<port>], proxies through a host, port defaults to 23\n"
msgstr ""
"/GATE <Host> [<Port>], benutzt den Host als Proxy. Der Parameter »Port« wird "
"auf 23 gesetzt, wenn nicht angegeben\n"

#: src/common/outbound.c:531
msgid "/HOP <nick>, gives chanhalf-op status to the nick (needs chanop)\n"
msgstr ""
"/HOP <Spitzname>, vergibt Halb-Operator-Status (\"chanhalf-op\") an den "
"Spitznamen (benötigt Operator-Status)\n"

#: src/common/outbound.c:533
msgid ""
"/IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    options - NOSAVE, QUIET\n"
msgstr ""
"/IGNORE <Maske> <Typen...> <Optionen...>\n"
"     Maske    - Hostmaske, welche Sie ignorieren möchten,                 "
"z.B. *!*@*.aol.com\n"
"     Typen    - Arten von Daten, welche Sie ignorieren möchten,              "
"   eine der folgenden oder »ALL«:\n"
"                PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"     Optionen - NOSAVE, QUIET\n"

#: src/common/outbound.c:540
msgid ""
"/INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)\n"
msgstr ""
"/INVITE <Spitzname> [<Kanal>], lädt jemanden in einen Kanal ein, "
"normalerweise in den Aktuellen (benötigt Operator-Status)\n"

#: src/common/outbound.c:541
msgid "/JOIN <channel>, joins the channel\n"
msgstr "/JOIN <Kanal>, diesen Kanal betreten\n"

#: src/common/outbound.c:543
msgid "/KICK <nick>, kicks the nick from the current channel (needs chanop)\n"
msgstr ""
"/KICK <Spitzname>, wirft den Spitznamen aus dem aktuellen Kanal (benötigt "
"Operator-Status)\n"

#: src/common/outbound.c:545
msgid ""
"/KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)\n"
msgstr ""
"/KICKBAN <Spitzname>, verbannt und wirft den Spitznamen aus dem aktuellen "
"Kanal (benötigt Operator-Status)\n"

#: src/common/outbound.c:547
msgid "/LAGCHECK, forces a new lag check\n"
msgstr ""
"/LAGCHECK, erzwingt eine neue Überprüfung der zeitlichen Verzögerung "
"(»Lag«)\n"

#: src/common/outbound.c:549
msgid "/LASTLOG <string>, searches for a string in the buffer.\n"
msgstr ""
"/LASTLOG <Zeichenkette>, durchsucht den Puffer nach der angegebenen "
"Zeichenkette.\n"

#: src/common/outbound.c:553
msgid "/LISTDLL, Lists all currenly loaded plugins\n"
msgstr "/LISTDLL, listed die zur Zeit geladenen Plugins auf\n"

#: src/common/outbound.c:555
msgid "/LOAD <file>, loads a Perl script\n"
msgstr "/LOAD <Datei>, lädt ein Perl-Skript\n"

#: src/common/outbound.c:557
msgid "/LOADDLL <file>, loads a plugin\n"
msgstr "/LOADDLL <Datei>, lädt ein Plugin\n"

#: src/common/outbound.c:561
msgid ""
"/MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)\n"
msgstr ""
"/MDEHOP, enfernt den Halb-Operator-Status (\"chanhalf-op\") von allen "
"Halb-Operatoren im aktuellen Kanal auf einmal (benötigt Operator-Status)\n"

#: src/common/outbound.c:563
msgid "/MDEOP, Mass deop's all chanops in the current channel (needs chanop)\n"
msgstr ""
"/MDEOP, enfernt den Operator-Status von allen Operatoren im aktuellen Kanal "
"auf einmal (benötigt Operator-Status)\n"

#: src/common/outbound.c:565
msgid ""
"/MKICK, Mass kicks everyone except you in the current channel (needs "
"chanop)\n"
msgstr ""
"/MKICK, wirft jeden ausser Ihnen aus dem aktuellen Kanal (benötigt "
"Operator-Status)\n"

#: src/common/outbound.c:567
msgid ""
"/MKICKB, Sets a ban of *@* and mass kicks everyone except you in the current "
"channel (needs chanop)\n"
msgstr ""
"/MKICK, setzt einen Bann auf *@* und wirft jeden ausser Ihnen aus dem "
"aktuellen Kanal (benötigt Operator-Status)\n"

#: src/common/outbound.c:569
msgid ""
"/ME <action>, sends the action to the current channel (actions are written "
"in the 3rd person, like /me jumps)\n"
msgstr ""
"/ME <Aktion>, sendet die Aktion in den aktuellen Kanal (Aktionen werden in "
"der dritten Person angegeben, z.B. /me springt\n"

#: src/common/outbound.c:570
msgid "/MSG <nick> <message>, sends a private message\n"
msgstr ""
"/MSG <Spitzname> <Nachricht>, sendet eine private Nachricht an den "
"Spitznamen\n"

#: src/common/outbound.c:573
msgid "/NAMES, Lists the nicks on the current channel\n"
msgstr "/NAMES, listet die Spitznamen im aktuellen Kanal auf\n"

#: src/common/outbound.c:575
msgid "/NCTCP <nick> <message>, Sends a CTCP notice\n"
msgstr ""
"/NCTCP <Spitzname> <Nachricht>, sendet eine CTCP-Notiz an den Spitznamen\n"

#: src/common/outbound.c:576
msgid "/NEWSERVER <hostname> [<port>]\n"
msgstr "/NEWSERVER <Hostname> [<Port>]\n"

#: src/common/outbound.c:577
msgid "/NICK <nickname>, sets your nick\n"
msgstr "/NICK <Spitzname>, setzt den eigenen Spitznamen\n"

#: src/common/outbound.c:580
msgid ""
"/NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to\n"
msgstr ""
"/NOTICE <Spitzname/Kanal> <Nachricht>, sendet eine Notiz. Notizen sind ein "
"Typ von Nachricht, auf die automatisch reagiert werden sollte\n"

#: src/common/outbound.c:582
msgid "/NOTIFY [<nick>], lists your notify list or adds someone to it\n"
msgstr ""
"/NOTIFY [<Spitzname>], zeigt die Ihre \"Notify-Liste\" an oder fügt jemanden "
"zu ihr hinzu\n"

#: src/common/outbound.c:584
msgid "/OP <nick>, gives chanop status to the nick (needs chanop)\n"
msgstr ""
"/OP <Spitzname>, vergibt Operator-Status an den Spitznamen (benötigt "
"Operator-Status)\n"

#: src/common/outbound.c:586
msgid ""
"/PART [<channel>] [<reason>], leaves the channel, by default the current "
"one\n"
msgstr ""
"/PART [<Kanal>] [<Grund>], verlässt den Kanal, normalerweisen den Aktuellen\n"

#: src/common/outbound.c:588
msgid "/PING <nick | channel>, CTCP pings nick or channel\n"
msgstr "/PING <Spitzname | Kanal>, CTCP pingt einen Spitznamen oder Kanal\n"

#: src/common/outbound.c:592
msgid "/PKILL <name>, kills the script of the given name\n"
msgstr "/PKILL <Name>, beendet das Skript mit dem gegebenen Namen\n"

#: src/common/outbound.c:593
msgid "/PLIST, lists the current python scripts\n"
msgstr "/PLIST, listet die aktivierten Python-Skripte auf\n"

#: src/common/outbound.c:594
msgid "/PLOAD loads a python script\n"
msgstr "/PLOAD lädt ein Python-Skript\n"

#: src/common/outbound.c:598
msgid "/QUERY <nick>, opens up a new privmsg window to someone\n"
msgstr ""
"/QUERY <Spitzname>, öffnet ein neues Dialog-Fenster zu dem Spitznamen\n"

#: src/common/outbound.c:600
msgid "/QUIT [<reason>], disconnects from the current server\n"
msgstr "/QUIT [<Grund>], beendet die Verbindung zum aktuellen Server\n"

#: src/common/outbound.c:602
msgid "/QUOTE <text>, sends the text in raw form to the server\n"
msgstr "/QUOTE <Text>, sendet den Text uninterpretiert (»raw«) an den Server\n"

#: src/common/outbound.c:605
msgid ""
"/RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as "
"/RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers\n"
msgstr ""
"/RECONNECT [-ssl] [<Host>] [<Port>] [<Passwort>], kann einfach als "
"/RECONNECT aufgerufen werden, um sich neu zum aktuellen Server zu verbinden "
"oder als /RECONNECT ALL, um sich neu zu allen Servern zu verbinden, die Sie "
"offen haben\n"

#: src/common/outbound.c:608
msgid ""
"/RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers\n"
msgstr ""
"/RECONNECT [<Host>] [<Port>] [<Passwort>], kann einfach als /RECONNECT "
"aufgerufen werden, um sich neu zum aktuellen Server zu verbinden oder als "
"/RECONNECT ALL, um sich neu zu allen Servern zu verbinden, die Sie offen "
"haben\n"

#: src/common/outbound.c:611
msgid "/RMDLL <dll name>, unloads a plugin\n"
msgstr "/RMDLL <dll-Name>, deaktiviert ein Plugin\n"

#: src/common/outbound.c:615
msgid "/SAY <text>, sends the text to the object in the current window\n"
msgstr "/SAY <Text>, sendet den Text zum Objekt im aktuellen Fenster\n"

#: src/common/outbound.c:618
msgid "/SCPINFO, Lists some information about current Perl bindings\n"
msgstr "/SCPINFO, zeigt Informationen über die Perlschnittstelle an \n"

#: src/common/outbound.c:620
msgid "/SET <variable> [<value>]\n"
msgstr "/SET <Variable> [<Wert>]\n"

#: src/common/outbound.c:624
msgid ""
"/SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel\n"
msgstr ""
"/SERVCHAN [-ssl] <Host> <Port> <Kanal>, verbindet zum Server und betritt  "
"den angegebenen Kanal\n"

#: src/common/outbound.c:627
msgid "/SERVCHAN <host> <port> <channel>, connects and joins a channel\n"
msgstr ""
"/SERVCHAN <Host> <Port> <Kanal>, verbindet zum Server und betritt  den "
"angegebenen Kanal\n"

#: src/common/outbound.c:631
msgid ""
"/SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the "
"default port is 6667 for normal connections, and 994 for ssl connections.\n"
msgstr ""
"/SERVER [-ssl] <Host> [<Port>] [<Passwort>], verbindet zu einem Server. Der "
"Standartport ist 6667 bein normalen, und 994 bei SSL-Verbindungen.\n"

#: src/common/outbound.c:634
msgid ""
"/SERVER <host> [<port>] [<password>], connects to a server, the default port "
"is 6667.\n"
msgstr ""
"/SERVER [-ssl] <Host> [<Port>] [<Passwort>], verbindet zu einem Server. Der "
"Standartport ist 6667.\n"

#: src/common/outbound.c:636
msgid "/TIMER <seconds> <command>\n"
msgstr ""
"/TIMER <Sekunden> <Befehl>, führt einen Befehl nach der angegebenen Zeit "
"aus\n"

#: src/common/outbound.c:638
msgid ""
"/TOPIC [<topic>], sets the topic if one is given, else shows the current "
"topic\n"
msgstr ""
"/TOPIC [<Thema>], setzt das Thema des Kanals, falls eines angegeben ist und "
"zeigt andernfalls das Aktuelle an\n"

#: src/common/outbound.c:639
msgid "/UNIGNORE <mask> [QUIET]\n"
msgstr ""
"/UNIGNORE <Maske> [QUIET], hebt das Ignorieren von Benutzern mit dieser "
"Maske auf\n"

#: src/common/outbound.c:641
msgid "/UNLOADALL, Unloads all perl scripts\n"
msgstr "/UNLOADALL, deaktiviert alle Perl-Skripten\n"

#: src/common/outbound.c:645
msgid ""
"/WALLCHOP <message>, sends the message to all chanops on the current "
"channel\n"
msgstr ""
"/WALLCHOP <Nachricht>, sendet die Nachricht zu allen Operatoren im aktuellen "
"Kanal\n"

#: src/common/outbound.c:647
msgid "/WALLCHAN <message>, writes the message to all channels\n"
msgstr "/WALLCHAN <Nachricht>, sendet die Nachricht an alle Kanäle\n"

#: src/common/outbound.c:649
msgid "/VOICE <nick>, gives voice status to someone (needs chanop)\n"
msgstr ""
"/VOICE <Spitzname>, vergibt Voice-Status an den Spitznamen (benötigt "
"Operator-Status)\n"

#: src/common/outbound.c:665
msgid "Usage:\n"
msgstr "Aufruf:\n"

