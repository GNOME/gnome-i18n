#: src/common/outbound.c:1635
#, c-format
msgid "Unknown arg '%s' ignored."
msgstr "Unbekannter Parameter »%s« ignoriert"

#: src/common/outbound.c:2313
msgid "No such plugin found.\n"
msgstr "Kein derartiges Plugin gefunden.\n"

#: src/common/outbound.c:2316 src/fe-gtk/plugingui.c:128
msgid "That plugin is refusing to unload.\n"
msgstr "Dieses Plugin verweigert das entfernen aus dem Speicher.\n"

#: src/common/outbound.c:2450
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr ""
"ADDBUTTON <Name> <Aktion>, einen Knopf unterhalb der Benutzerliste hinzufügen"

#: src/common/outbound.c:2452
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr ""
"ALLCHAN <Befehl>, einen Befehl an alle Kanäle, in denen Sie sich aufhalten, "
"senden"

#: src/common/outbound.c:2454
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr ""
"ALLSERV <Befehl>, einen Befehl an alle Server, mit denen Sie verbunden sind, "
"senden"

#: src/common/outbound.c:2455
msgid "AWAY [<reason>], sets you away"
msgstr "AWAY [<Grund>], Sie auf abwesend setzen"

#: src/common/outbound.c:2457
msgid ""
"BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)"
msgstr ""
"BAN <Maske> [<Banntyp>], jeden, der auf die Maske zutrifft aus dem aktuellen "
"Kanal verbannen. Falls sie bereits im Kanal sind, werden sie hierdurch nicht "
"gekickt (benötigt Kanal-OP)"

#: src/common/outbound.c:2459
msgid "UNBAN <mask> [<mask>...], unbans the specified masks."
msgstr "UNBAN <Maske> [<Maske>...], den Bann der angegebenen Masken aufheben"

#: src/common/outbound.c:2460
msgid "CLEAR, Clears the current text window"
msgstr "CLEAR, das aktuelle Textfenster leeren"

#: src/common/outbound.c:2461
msgid "CLOSE, Closes the current window/tab"
msgstr "CLOSE, das aktuelle Fenster/den aktuellen Reiter schließen"

#: src/common/outbound.c:2464
msgid ""
"CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO"
msgstr ""
"CTCP <Spitzname> <Nachricht>, die CTCP-Nachricht an den Spitznamen senden, "
"übliche Nachrichten sind VERSION oder USERINFO"

#: src/common/outbound.c:2466
msgid "COUNTRY <code>, finds a country code, eg: au = australia"
msgstr "COUNTRY <Code>, einen Ländercode suchen, z.B.: au = Australien"

#: src/common/outbound.c:2468
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "CYCLE, den aktuellen Kanal verlassen und unmittelbar neu betreten"

#: src/common/outbound.c:2470
msgid ""
"\n"
"DCC GET <nick>          - receive an offered file\n"
"DCC SEND <nick> <file>  - send a file to someone\n"
"DCC LIST                - show DCC list\n"
"DCC CHAT <nick>         - offer DCC CHAT to someone\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr ""
"\n"
"DCC GET <Spitzname>          - eine angebotene Datei empfangen\n"
"DCC SEND <Spitzname> <Datei>  - eine Datei an jemanden schicken\n"
"DCC LIST                - DDC-Liste anzeigen\n"
"DCC CHAT <Spitzname>         - jemandem DDC-Chat anbieten\n"
"DCC CLOSE <Typ> <Spitzname> <Datei>         Beispiel:\n"
"         /dcc close send heinzrudolfkunze datei.tar.gz"

#: src/common/outbound.c:2479
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr "DELBUTTON <name>, einen Knopf unterhalb der Benutzerliste löschen"

#: src/common/outbound.c:2481
msgid ""
"DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)"
msgstr ""
"DEHOP <Spitzname>, Kanal-Half-OP-Status des Spitznamen entfernen (benötigt "
"Kanal-OP)"

#: src/common/outbound.c:2483
msgid ""
"DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEOP <Spitzname>, Kanal-OP des Spitznamen entfernen (benötigt Kanal-OP)"

#: src/common/outbound.c:2485
msgid ""
"DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEVOICE <Spitzname>, Voice-Status des Spitznamens im aktuellen Kanal "
"entfernen (erfordert Kanal-OP)"

#: src/common/outbound.c:2486
msgid "DISCON, Disconnects from server"
msgstr "DISCON, Verbindung zum Server trennen"

#: src/common/outbound.c:2487
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr ""
"DNS <Spitzname|Rechner|IP>, Die IP-Adresse eines benutzers herausfinden"

#: src/common/outbound.c:2488
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <Text>, Text lokal ausgeben"

#: src/common/outbound.c:2491
msgid ""
"EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box"
msgstr ""
"EXEC [-o] <Befehl>, den Befehl ausführen. Falls die »-o«-Option gesetzt ist, "
"wird die Ausgabe an den aktuellen Kanal gesendet, ansonsten wird sie im "
"aktuellen Textfeld ausgegeben."

#: src/common/outbound.c:2493
msgid ""
"EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed"
msgstr ""
"EXECKILL [-9], eine laufende ausführbare Datei in der aktuellen Sitzung "
"abwürgen. Falls »-9« gesetzt ist, wird der Prozess SIGgeKILLt"

#: src/common/outbound.c:2495
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "EXECSTOP, SIGSTOP an den Prozess senden"

#: src/common/outbound.c:2496
msgid "EXECCONT, sends the process SIGCONT"
msgstr "EXECCONT, SIGCONT an den Prozess senden"

#: src/common/outbound.c:2497
msgid "EXECWRITE, sends data to the processes stdin"
msgstr "EXECWRITE, Daten an die Prozess-Standardeingabe senden"

#: src/common/outbound.c:2501
msgid "FLUSHQ, flushes the current server's send queue"
msgstr "FLUSHQ, die Sende-Warteschlange des aktuellen Servers leeren"

#: src/common/outbound.c:2503
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr ""
"GATE <Rechner> [<Port>], Verbindung über einen Proxy aufbauen, Port-Vorgabe "
"auf 23"

#: src/common/outbound.c:2506
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr ""
"HOP <Spitzname>, Kanal-Half-OP-Status an den Spitznamen geben (benötigt "
"Kanal-OP)"

#: src/common/outbound.c:2508
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr ""
"IGNORE <Maske> <Typen...> <Optionen...>\n"
"    Maske - Zu ignorierende Rechner maske, z.B.: *!*@*.aol.com\n"
"    Typen - Zu ignorierende Datentypen, einer oder alle von:\n"
"            PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    Optionen - NOSAVE, QUIET"

#: src/common/outbound.c:2515
msgid ""
"INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)"
msgstr ""
"INVITE <Spitzname> [<Kanal>], jemanden in einen Kanal einladen, als Vorgabe "
"den aktuellen Kanal (erfordert Kanal-OP)"

#: src/common/outbound.c:2516
msgid "JOIN <channel>, joins the channel"
msgstr "JOIN <Kanal>, den Kanal betreten"

#: src/common/outbound.c:2518
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr ""
"KICK <Spitznem>, die Person, der der Spitzname gehört, vom aktuellen Kanal "
"kicken (erfordert Kanal-OP)"

#: src/common/outbound.c:2520
msgid ""
"KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)"
msgstr ""
"KICK <Spitznem>, die Person, der der Spitzname gehört, vom aktuellen Kanal "
"verbannen (erfordert Kanal-OP)"

#: src/common/outbound.c:2522
msgid "LAGCHECK, forces a new lag check"
msgstr "LAGCHECK, einen erneuten Lag-Check erzwingen"

#: src/common/outbound.c:2524
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr "LASTLOG <Zeichenkette>, den Puffer nach einer Zeichenkette durchsuchen"

#: src/common/outbound.c:2526
msgid "LOAD <file>, loads a plugin or script"
msgstr "LOAD <Datei>, ein Plugin oder Skript laden"

#: src/common/outbound.c:2529
msgid ""
"MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr ""
"MDEHOP, den Half-OP-Status aller Half-OPs im aktuellen Kanal entfernen "
"(erfordert Kanal-OP)"

#: src/common/outbound.c:2531
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr ""
"MDEOP, den OP-Status aller OPs im aktuellen Kanal entfernen (erfordert Kanal-"
"OP)"

#: src/common/outbound.c:2533
msgid ""
"MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr "MKICK, alle außer Ihnen im aktuellen Kanal kicken (erfordert Kanal-OP)"

#: src/common/outbound.c:2535
msgid ""
"ME <action>, sends the action to the current channel (actions are written in "
"the 3rd person, like /me jumps)"
msgstr ""
"ME <Aktion>, die Aktion an den aktuellen Kanal senden (Aktionen werden in "
"der dritten Person geschrieben, wie /me springt)"

#: src/common/outbound.c:2536
msgid "MSG <nick> <message>, sends a private message"
msgstr "MSG <Spitzname> <Nachricht>, eine private Nachricht senden"

#: src/common/outbound.c:2539
msgid "NAMES, Lists the nicks on the current channel"
msgstr ""
"NAMES, die Spitznamen der im aktuellen Kanal befindlichen Personen auflisten"

#: src/common/outbound.c:2541
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <Spitzname> <Nachricht>, Eine CTCP-Meldung senden"

#: src/common/outbound.c:2542
msgid "NEWSERVER <hostname> [<port>]"
msgstr "NEWSERVER <Rechnername> [<Port>]"

#: src/common/outbound.c:2543
msgid "NICK <nickname>, sets your nick"
msgstr "NICK <Spitzname>, Ihren Spitznamen festlegen"

#: src/common/outbound.c:2546
msgid ""
"NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to"
msgstr ""
"NOTICE <Spitzname/Kanal> <Nachricht>, Eine Notiz senden. Notizen sind ein "
"Nachrichtentyp, auf den automatisch reagiert werden sollte"

#: src/common/outbound.c:2548
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr ""
"NOTIFY [<Spitzname>], Ihre Benachrichtigungsliste auflisten oder jemanden zu "
"dieser hinzufügen"

#: src/common/outbound.c:2550
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr "OP <Spitzname>, dem Spitznamen Kanal-OP entfernen (benötigt Kanal-OP)"

#: src/common/outbound.c:2552
msgid ""
"PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr ""
"PART [<Kanal>] [<Grund>], den Kanal verlassen, als Vorgabe den Aktuellen"

#: src/common/outbound.c:2554
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr ""
"PING <Spitzname | Kanal>, CTCP-Ping an Spitznamen oder Kanal durchführen"

#: src/common/outbound.c:2556
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr ""
"QUERY <Spitzname>, ein neues, privates Konversationsfenster mit jemandem "
"öffnen"

#: src/common/outbound.c:2558
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "QUIT [<Grund>], Verbindung mit dem aktuellen Server trennen"

#: src/common/outbound.c:2560
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "QUOTE <Text>, den Text in Rohform an den Server senden"

#: src/common/outbound.c:2563
msgid ""
"RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers"
msgstr ""
"RECONNECT [-ssl] [<Rechner>] [<Port>] [<Passwort>], kann nur als /RECONNECT "
"aufgerufen werden, um mit dem aktuellen Server neu zu verbinden oder mit /"
"RECONNECT ALL, um die Verbindung mit allen geöffneten Servern neu aufzubauen"

#: src/common/outbound.c:2566
msgid ""
"RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers"
msgstr ""
"RECONNECT [<Rechner>] [<Port>] [<Passwort>], kann nur als /RECONNECT "
"aufgerufen werden, um mit dem aktuellen Server neu zu verbinden oder mit /"
"RECONNECT ALL, um die Verbindung mit allen geöffneten Servern neu aufzubauen"

#: src/common/outbound.c:2568
msgid ""
"RECV <text>, send raw data to xchat, as if it was received from the irc "
"server"
msgstr ""
"RECV <Text>, Rohdaten an X-Chat senden, als ob sie vom IRC-Server empfangen "
"worden wären"

#: src/common/outbound.c:2571
msgid "SAY <text>, sends the text to the object in the current window"
msgstr "SAY <Text>, den Text an das Objekt im aktuellen Fenster senden"

#: src/common/outbound.c:2572
msgid "SET <variable> [<value>]"
msgstr "SET <Variable> [<Wert>]"

#: src/common/outbound.c:2576
msgid "SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel"
msgstr ""
"SERVCHAN [-ssl] <Rechner> <Port> <Kanal>, verbinden und einen Kanal betreten"

#: src/common/outbound.c:2579
msgid "SERVCHAN <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN <Rechner> <Port> <Kanal>, verbinden und einen Kanal betreten"

#: src/common/outbound.c:2583
msgid ""
"SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the "
"default port is 6667 for normal connections, and 9999 for ssl connections"
msgstr ""
"SERVER [-ssl] <Rechner> [<Port>] [<Passwort>], mit einem Server verbinden, "
"der vorgegebene Port ist 6667 für normale und 9999 für SSL-Verbindungen"

#: src/common/outbound.c:2586
msgid ""
"SERVER <host> [<port>] [<password>], connects to a server, the default port "
"is 6667"
msgstr ""
"SERVER <Rechner> [<Port>] [<Passwort>], mit einem Server verbinden, der "
"vorgegebene Port ist 6667"

#: src/common/outbound.c:2589
msgid ""
"TOPIC [<topic>], sets the topic if one is given, else shows the current topic"
msgstr ""
"TOPIC [<Thema>], das Thema festlegen, falls eines gegeben ist, ansonsten das "
"aktuelle Thema anzeigen"

#: src/common/outbound.c:2590
msgid "UNIGNORE <mask> [QUIET]"
msgstr "UNIGNORE <Maske> [QUIET]"

#: src/common/outbound.c:2591
msgid "UNLOAD <name>, unloads a plugin or script"
msgstr "UNLOAD <Name>, ein Plugin oder Skript aus dem Speicher entfernen"

#: src/common/outbound.c:2594
msgid ""
"WALLCHOP <message>, sends the message to all chanops on the current channel"
msgstr ""
"WALLCHOP <message>, die Nachricht an alle Kanal-OPs im aktuellen Kanal "
"schicken"

#: src/common/outbound.c:2596
msgid "WALLCHAN <message>, writes the message to all channels"
msgstr "WALLCHAN <message>, die Nachricht an alle Kanäle senden"

#: src/common/outbound.c:2598
msgid "VOICE <nick>, gives voice status to someone (needs chanop)"
msgstr "VOICE <Spitzname>,  Jemandem Voice-Status geben (erfordert Kanal-OP)"

#: src/common/outbound.c:2618
#, c-format
msgid "Usage: %s\n"
msgstr "Verwendung: %s\n"

